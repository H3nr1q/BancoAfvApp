<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.SelectDatabaseDialogFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="16dp">

        <Button
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/bntSelectDatabase1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Banco 1"/>

        <Button
            app:layout_constraintTop_toBottomOf="@+id/bntSelectDatabase1"
            android:id="@+id/bntSelectDatabase2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Banco 2"/>
        <Button
            app:layout_constraintTop_toBottomOf="@+id/bntSelectDatabase2"
            android:id="@+id/bntSelectDatabase3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Banco 3"/>



    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>